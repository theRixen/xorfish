function XOR:CopyEncryptedCodeToClipboard()
local f = "ABCDEFGHIJKLMNOPRSTUVWXYZabcdefghijklmnoprstuvwxyz1234567890!@#$%^&*()_-+=}]{[':;.>,</?~'ÙŠØºÙØ± Ø§Ù„Ù„Ù‡ Ù„Ø¬Ù…ÙŠØ¹ Ø§Ù„Ø®Ø·Ø§Ø© ØŒ Ø¨Ù…Ø§ ÙÙŠ Ø°Ù„Ùƒ Ø£ÙˆÙ„Ø¦Ùƒ Ø§Ù„Ø°ÙŠÙ† Ø­Ø§ÙˆÙ„ÙˆØ§ ØªÙƒØ³ÙŠØ± Ù…Ø§ Ù„Ø§ ÙŠÙ…ÙƒÙ† ØªØµØ¯Ø¹Ù‡ÓÓƒÒ©Ô–Ô”Ô á‹¢á‹„á‹ â‚©à¸¿â‚ªÎ£á‹ˆá‹° áˆ¥áŠáˆáŒá‰£áˆ­ á‰¤á‰± áŠ¥áŠ•áŠ³áŠ• á‰ á‹°áˆ…áŠ“ áˆ˜áŒ¡á¢ áˆáŒ†á‰¹áŠ• áˆˆáŠ áˆáˆ‹áŠ«á‰½áŠ• á‹¨áˆáŠ•áˆ á‹‹ð”ð“¾ð“®ð“±ð“¾ð“ªð“¹ð“²ð“ªð“¸ð“¹ð“²ð“ªð“¸ð“«ð“®ð“²ð“¯ð“®ð“·ð“°ð”ð“²ð“ªð“¸ð”ð“²ð“ªð“¸á‰ á‰µ á‰¦á‰³ á‹­áˆ… áŠá‹ á¡á¡ áŠ¥á‹áŠá‰°áŠ›á‹ áŠ á‹³áŠ áŠ¥áŒá‹šáŠ á‰¥áˆ”áˆ­ áŠ¥áŠ•á‹°áˆ†áŠ áŠ¥áŠ“áˆáŠ“áˆˆáŠ• áŠ¥áŠ“áˆ áŠ¨á‹˜áˆ‹áˆˆáˆ›á‹Š áˆ²áŠ¦áˆ á‹«á‹µáŠ“áˆáŒ†á‹­ áŒƒáŠ• á‹¨áˆ™áŠ¨áˆ« áˆ­á‹•áˆ° áŒ‰á‹³á‹­ á‹¨áŒˆá‰¢á‹« áŠáƒáŠá‰µ á–áˆŠáˆµ áŠ¥áŠ” áŠ áˆ…á‹«á‹áŠ• á‹­á‰ áˆ‹áˆ‰ áŠ¥áŒ áˆ‹áˆƒáˆˆáˆ áˆ‹áˆ‹á‰½áˆ… áˆ³á‰…áŒ†á‹­ áŒƒáŠ• á‹¨áˆ™áŠ¨áˆ« áˆ­á‹•áˆ° áŒ‰á‹³á‹­ á‹¨áŒˆá‰¢á‹« áŠáƒáŠá‰µ á–áˆŠáˆµ áŠ¥áŠ” áŠ áˆ…á‹«á‹áŠ• á‹­á‰ áˆ‹áˆ‰ à¨®à©ˆà¨‚ à¨•à¨¾à¨²à©€ à¨²à©€à¨¨à¨•à¨¸ à¨¬à¨¾à¨ˆà¨¨à¨°à©€ à¨¸à©‹à¨°à¨¸ à¨•à©‹à¨¡ à¨šà¨¾à¨¹à©à©°à¨¦à¨¾ à¨¹à¨¾à¨‚! áŠ¥áŒ áˆ‹áˆƒáˆˆáˆ áˆ‹áˆ‹á‰½áˆ… áˆ³á‰… á¡á¡"
local function generate(digits, text)
    local g = text
    local enc1 = syn.crypt.custom.encrypt(
    "bf-cbc",
    text,
    "Ø§Ù„Ø°ÙŠÙ† Ø³ÙŠØ± Ù…Ø§",
    "rÃ»â€“as")
    syn.crypt.encrypt(syn.crypt.encrypt(text, key2..key1..key8), syn.crypt.encrypt(key8, key8..key9))
        for _ = 1, digits do
            local i = math.random(1, #f)
            g = g..f:sub(i, i)
        end
        text = enc1..enc2
        return g
    end
    local function generatenumber(number)
            local t = "enter text here"
            local a
        for _ = 1, number do
        function a()
            for v = 1,5 do -- this is where the fun begins; encrypts crypt encrypt and bashes the keys close each other so it's a pain in the ass to reverse engineer
            string.gsub(
            syn.crypt.encrypt(syn.crypt.encrypt(t, key3..key4..key5..key6), key3..key10),
            syn.crypt.encrypt(f, syn.crypt.encrypt(key5, key7..key5)),
            syn.crypt.encrypt(t, syn.crypt.encrypt(key7, key3))..syn.crypt.encrypt(f, enc3)
            )
            end
        end
        a()
        t = enc1
        setclipboard(generate(1992, t)) -- t is the text, "1992" is the character amount
        end
    end
    print("Encrypted successfully in " .. time / tick() .. " second(s)")
    generatenumber(1)
    local hint = Instance.new("Hint", game.Workspace)
    hint.Text = "Copied encrypted text to clipboard!"
    wait(5)
    hint:Destroy()
end
